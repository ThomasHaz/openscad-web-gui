{
  "name": "openscad-web-gui",
  "version": "0.0.1",
  "description": "Web GUI for Openscad WASM",
  "author": "",
  "browser": "src/app.tsx",
  "scripts": {
    "build": "node esbuild.mjs",
    "corsproxy": "scripts/cors-proxy.js",
    "dev": "NODE_OPTIONS=--trace-warnings CORSPROXY=http://localhost:8888/ node esbuild.mjs --serve --watch --cors-proxy",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier": "prettier . --write",
    "serve": "node esbuild.mjs --serve",
    "test": "jest",
    "test:e2e": "cypress run",
    "watch": "node esbuild.mjs --watch"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "*",
    "@types/jest": "*",
    "@types/three": "*",
    "@typescript-eslint/eslint-plugin": "*",
    "cors-anywhere": "*",
    "cypress": "*",
    "cypress-multi-reporters": "*",
    "cypress-terminal-report": "*",
    "cypress-visual-regression": "*",
    "esbuild": "*",
    "esbuild-plugin-copy": "*",
    "esbuild-plugin-replace": "*",
    "eslint": "*",
    "eslint-plugin-react": "*",
    "prettier": "*",
    "ts-jest": "*",
    "http-proxy-node16": "1.0.6"
  },
  "overrides": {
    "cors-anywhere": {
      "http-proxy": "npm:http-proxy-node16@1.0.6"
    },
    "esbuild-plugin-replace": {
      "magic-string": "npm:magic-string@0.30.17"
    },
    "babel-plugin-istanbul": {
      "test-exclude": "npm:test-exclude@7.0.1"
    }
  },
  "dependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@fontsource/roboto": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "@mui/x-tree-view": "*",
    "@react-three/drei": "*",
    "@react-three/fiber": "*",
    "@zip.js/zip.js": "*",
    "mui-chips-input": "*",
    "notistack": "*",
    "react": "*",
    "react-dom": "*",
    "react-dropzone": "*",
    "three": "*"
  }
}
